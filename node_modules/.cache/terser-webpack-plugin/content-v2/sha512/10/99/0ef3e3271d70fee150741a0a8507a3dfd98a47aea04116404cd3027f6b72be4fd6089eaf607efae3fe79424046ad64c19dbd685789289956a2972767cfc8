as <span><input type=\"file\" accept=\"text/plain\" onChange={(e) => {\n                const file = e.target.files[0];\n                setFile(file);\n            }} /></span></h1>\n        </DivConfig>\n\n        <Div ref={div}>\n            <h1>Banca <span>{ balance ? `$${balance}` : 'Loading...' }</span></h1>\n            <h1>Delay <span>{ delay }</span></h1>\n            <h1>Stop Win <span>{ stopWin }</span></h1>\n            <h1>Stop Loss <span>{ stopLoss }</span></h1>\n            <h1>MartinGales <span>{ martinGales.map((value) => {\n                return `${value}\\n`;\n            }) }</span></h1>\n        </Div>\n\n        <StartBot>\n            <button ref={buttonStart} onClick={handleClick}>Start</button>\n        </StartBot>\n        </>\n    );\n}\n","import React from 'react';\nimport { Switch, Route } from 'react-router-dom';\n\nimport LoginIQ from '../pages/LoginIQ';\nimport Main from '../pages/Main';\n\nexport default function Routes() {\n    return (\n        <Switch>\n            <Route exact path=\"/\" component={LoginIQ} />\n            <Route exact path=\"/main\" component={Main} />\n        </Switch>\n    );\n    \n}","import * as types from './types';\n\nconst initialState = {\n    email: '',\n    password: '',\n    ssid: ''\n};\n\nfunction reducer(state=initialState, action: {\n    type: string;\n    payload: typeof initialState;\n}): typeof initialState {\n    switch(action.type) {\n        case types.LOGIN_IQOPTION: \n            const newState = action.payload;\n            return newState;\n            \n        case types.LOGOUT_IQOPTION:\n            return initialState;\n\n        default:\n            return state;\n    }\n}\n\nexport default reducer;","import { combineReducers } from 'redux';\n\nimport iqoption from './iqoption/reducer';\n\nexport default combineReducers({\n    iqoption\n});","import rootReducer from './modules/rootReducer';\n\nimport { createStore } from 'redux';\nimport { persistStore, persistReducer } from 'r